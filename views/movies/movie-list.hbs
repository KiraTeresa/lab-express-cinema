
<section id="movie-list">
    <h2>Ironhack Cinema</h2>
<div>
{{#each movies}}
<div class="movie-box">
    <div>
        <img src="{{image}}">
        <p>{{title}}</p>
    </div>
    <div class="button">
        <div class="inner-border-btn">
        <a href="/movies/{{_id}}">See more</a>
        </div>
    </div>
</div>
{{/each}}
</div>
</section>